{
  "manifest_version": 2,
  "name": "AV History Killer",
  "description": "Chrome Extension which clears AV browsing history automatically.",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": {
      "16": "img/icon-16.png",
      "48": "img/icon-48.png",
      "128": "img/icon-128.png"
    },
    "default_title": "AV History Killer",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
